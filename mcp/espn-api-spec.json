  "components": {
    "parameters": {
      "sport": {
        "name": "sport",
        "in": "path",
        "required": true,
        "description": "Sport type",
        "schema": {"type": "string", "enum": ["football", "basketball", "baseball", "hockey", "soccer", "golf", "racing", "tennis"]}
      },
      "league": {
        "name": "league",
        "in": "path",
        "required": true,
        "description": "League identifier",
        "schema": {"type": "string"},
        "examples": {
          "nfl": {"value": "nfl"},
          "college-football": {"value": "college-football"},
          "nba": {"value": "nba"},
          "wnba": {"value": "wnba"},
          "mens-college-basketball": {"value": "mens-college-basketball"},
          "mlb": {"value": "mlb"},
          "nhl": {"value": "nhl"},
          "eng.1": {"value": "eng.1", "summary": "Premier League"},
          "usa.1": {"value": "usa.1", "summary": "MLS"}
        }
      },
      "teamId": {
        "name": "teamId",
        "in": "path",
        "required": true,
        "description": "Team ID (numeric) or abbreviation",
        "schema": {"type": "string"}
      },
      "athleteId": {
        "name": "athleteId",
        "in": "path",
        "required": true,
        "description": "Athlete/Player ID",
        "schema": {"type": "integer"}
      },
      "eventId": {
        "name": "eventId",
        "in": "path",
        "required": true,
        "description": "Game/Event ID",
        "schema": {"type": "string"}
      },
      "year": {
        "name": "year",
        "in": "path",
        "required": true,
        "description": "Season year",
        "schema": {"type": "integer"},
        "example": 2024
      },
      "seasontype": {
        "name": "seasontype",
        "in": "path",
        "required": true,
        "description": "Season type: 1=preseason, 2=regular, 3=postseason",
        "schema": {"type": "integer", "enum": [1, 2, 3]}
      },
      "providerId": {
        "name": "providerId",
        "in": "path",
        "required": true,
        "description": "Betting provider ID",
        "schema": {"type": "integer"},
        "examples": {
          "caesars": {"value": 38},
          "williamhill": {"value": 31},
          "bet365": {"value": 2000}
        }
      },
      "dates": {
        "name": "dates",
        "in": "query",
        "description": "Date filter (YYYYMMDD, YYYYMMDD-YYYYMMDD, or YYYY)",
        "schema": {"type": "string"},
        "example": "20240115"
      },
      "limit": {
        "name": "limit",
        "in": "query",
        "description": "Maximum items to return",
        "schema": {"type": "integer", "default": 25}
      },
      "fantasyView": {
        "name": "view",
        "in": "query",
        "description": "Fantasy data view type",
        "schema": {
          "type": "string",
          "enum": ["mTeam", "mRoster", "mMatchup", "mSettings", "mDraftDetail", "mLiveScoring", "mPendingTransactions", "kona_player_info", "players_wl", "proTeamSchedules_wl"]
        }
      }
    },
    "schemas": {
      "SportsListResponse": {
        "type": "object",
        "properties": {
          "sports": {
            "type": "array",
            "items": {"$ref": "#/components/schemas/Sport"}
          }
        }
      },
      "Sport": {
        "type": "object",
        "properties": {
          "id": {"type": "string"},
          "uid": {"type": "string"},
