          "height": {"type": "integer"},
          "rel": {"type": "array", "items": {"type": "string"}}
        }
      },
      "Venue": {
        "type": "object",
        "properties": {
          "id": {"type": "string"},
          "fullName": {"type": "string"},
          "address": {"type": "object", "properties": {"city": {"type": "string"}, "state": {"type": "string"}}},
          "indoor": {"type": "boolean"}
        }
      },
      "Status": {
        "type": "object",
        "properties": {
          "clock": {"type": "number"},
          "displayClock": {"type": "string"},
          "period": {"type": "integer"},
          "type": {
            "type": "object",
            "properties": {
              "id": {"type": "string"},
              "name": {"type": "string", "enum": ["STATUS_SCHEDULED", "STATUS_IN_PROGRESS", "STATUS_FINAL"]},
              "state": {"type": "string", "enum": ["pre", "in", "post"]},
              "completed": {"type": "boolean"}
            }
          }
        }
      },
      "Odds": {
        "type": "object",
        "properties": {
          "provider": {"type": "object", "properties": {"id": {"type": "string"}, "name": {"type": "string"}}},
          "details": {"type": "string"},
          "overUnder": {"type": "number"},
          "spread": {"type": "number"},
          "homeTeamOdds": {"type": "object"},
          "awayTeamOdds": {"type": "object"}
        }
      },
      "NewsResponse": {
        "type": "object",
        "properties": {
          "header": {"type": "string"},
          "articles": {"type": "array", "items": {"$ref": "#/components/schemas/Article"}}
        }
      },
      "Article": {
        "type": "object",
        "properties": {
          "headline": {"type": "string"},
          "description": {"type": "string"},
          "published": {"type": "string", "format": "date-time"},
          "lastModified": {"type": "string", "format": "date-time"},
          "images": {"type": "array", "items": {"type": "object", "properties": {"url": {"type": "string", "format": "uri"}}}},
          "links": {"type": "object"}
        }
      },
      "Athlete": {
        "type": "object",
        "properties": {
          "id": {"type": "string"},
          "uid": {"type": "string"},
          "fullName": {"type": "string"},
          "displayName": {"type": "string"},
          "firstName": {"type": "string"},
          "lastName": {"type": "string"},
          "jersey": {"type": "string"},
          "position": {"type": "object", "properties": {"abbreviation": {"type": "string"}, "name": {"type": "string"}}},
          "team": {"$ref": "#/components/schemas/Team"},
          "headshot": {"type": "object", "properties": {"href": {"type": "string", "format": "uri"}}}
        }
      }
    }
  }
