{
  "openapi": "3.0.3",
  "info": {
    "title": "The Odds API",
    "description": "Live and historical sports odds from multiple bookmakers. Get odds for NFL, NBA, MLB, NHL, soccer, and many more sports.",
    "version": "4",
    "contact": {
      "name": "The Odds API",
      "email": "team@the-odds-api.com",
      "url": "https://the-odds-api.com"
    },
    "termsOfService": "https://the-odds-api.com/terms-and-conditions.html"
  },
  "servers": [
    {
      "url": "https://api.the-odds-api.com",
      "description": "Production server"
    },
    {
      "url": "https://ipv6-api.the-odds-api.com",
      "description": "IPv6 Production server"
    }
  ],
  "security": [
    {
      "apiKey": []
    }
  ],
  "paths": {
    "/v4/sports": {
      "get": {
        "operationId": "getSports",
        "summary": "Get Sports",
        "description": "Returns a list of in-season sport objects. The sport key can be used as the sport parameter in other endpoints. This endpoint does not count against the usage quota.",
        "tags": ["Sports"],
        "parameters": [
          {
            "$ref": "#/components/parameters/apiKey"
          },
          {
            "name": "all",
            "in": "query",
            "description": "If set to true, a list of both in and out of season sports will be returned",
            "schema": {
              "type": "boolean",
              "default": false
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "headers": {
              "x-requests-remaining": {
                "$ref": "#/components/headers/x-requests-remaining"
              },
              "x-requests-used": {
                "$ref": "#/components/headers/x-requests-used"
              },
              "x-requests-last": {
                "$ref": "#/components/headers/x-requests-last"
              }
            },
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Sport"
                  }
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "429": {
            "$ref": "#/components/responses/RateLimited"
          }
        }
      }
    },
    "/v4/sports/{sport}/odds": {
      "get": {
        "operationId": "getOdds",
        "summary": "Get Odds",
        "description": "Returns a list of upcoming and live games with recent odds for a given sport, region and market",
        "tags": ["Current Events"],
        "parameters": [
          {
            "$ref": "#/components/parameters/sport"
          },
          {
            "$ref": "#/components/parameters/apiKey"
          },
          {
            "$ref": "#/components/parameters/regions"
          },
          {
            "$ref": "#/components/parameters/markets"
          },
          {
            "$ref": "#/components/parameters/dateFormat"
          },
          {
            "$ref": "#/components/parameters/oddsFormat"
          },
          {
            "$ref": "#/components/parameters/eventIds"
          },
          {
            "$ref": "#/components/parameters/bookmakers"
          },
          {
            "$ref": "#/components/parameters/commenceTimeFrom"
          },
          {
            "$ref": "#/components/parameters/commenceTimeTo"
          },
          {
            "name": "includeLinks",
            "in": "query",
            "description": "If true, the response will include bookmaker links to events, markets, and betslips if available",
            "schema": {
              "type": "boolean",
              "default": false
            }
          },
          {
            "name": "includeSids",
            "in": "query",
            "description": "If true, the response will include source ids (bookmaker ids) for events, markets and outcomes if available",
            "schema": {
              "type": "boolean",
              "default": false
            }
          },
          {
            "name": "includeBetLimits",
            "in": "query",
            "description": "If true, the response will include the bet limit of each betting option",
            "schema": {
              "type": "boolean",
              "default": false
            }
          },
          {
            "name": "includeRotationNumbers",
            "in": "query",
            "description": "If true, the response will include home and away rotation numbers if available",
            "schema": {
              "type": "boolean",
              "default": false
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "headers": {
              "x-requests-remaining": {
                "$ref": "#/components/headers/x-requests-remaining"
              },
              "x-requests-used": {
                "$ref": "#/components/headers/x-requests-used"
              },
              "x-requests-last": {
                "$ref": "#/components/headers/x-requests-last"
              }
            },
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Event"
                  }
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "429": {
            "$ref": "#/components/responses/RateLimited"
          }
        }
      }
    },
    "/v4/sports/{sport}/scores": {
      "get": {
        "operationId": "getScores",
        "summary": "Get Scores",
        "description": "Returns a list of upcoming, live and recently completed games for a given sport. Live and recently completed games contain scores.",
        "tags": ["Current Events"],
        "parameters": [
          {
            "$ref": "#/components/parameters/sport"
          },
          {
            "$ref": "#/components/parameters/apiKey"
          },
          {
            "name": "daysFrom",
            "in": "query",
            "description": "The number of days in the past from which to return completed games. Valid values are 1-3.",
            "schema": {
              "type": "integer",
              "minimum": 1,
              "maximum": 3
            }
          },
          {
            "$ref": "#/components/parameters/dateFormat"
          },
          {
            "$ref": "#/components/parameters/eventIds"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "headers": {
              "x-requests-remaining": {
                "$ref": "#/components/headers/x-requests-remaining"
              },
              "x-requests-used": {
                "$ref": "#/components/headers/x-requests-used"
              },
              "x-requests-last": {
                "$ref": "#/components/headers/x-requests-last"
              }
            },
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/ScoreEvent"
                  }
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "429": {
            "$ref": "#/components/responses/RateLimited"
          }
        }
      }
    },
