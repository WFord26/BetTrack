  },
  "components": {
    "securitySchemes": {
      "apiKey": {
        "type": "apiKey",
        "in": "query",
        "name": "apiKey",
        "description": "API key for authentication"
      }
    },
    "parameters": {
      "apiKey": {
        "name": "apiKey",
        "in": "query",
        "required": true,
        "description": "The API key associated with your subscription",
        "schema": {
          "type": "string"
        }
      },
      "sport": {
        "name": "sport",
        "in": "path",
        "required": true,
        "description": "The sport key obtained from the /sports endpoint. Use 'upcoming' for next 8 games across all sports.",
        "schema": {
          "type": "string"
        }
      },
      "eventId": {
        "name": "eventId",
        "in": "path",
        "required": true,
        "description": "The event ID",
        "schema": {
          "type": "string"
        }
      },
      "regions": {
        "name": "regions",
        "in": "query",
        "required": true,
        "description": "Comma-separated bookmaker regions: us, us2, uk, au, eu",
        "schema": {
          "type": "string"
        },
        "example": "us,uk"
      },
      "markets": {
        "name": "markets",
        "in": "query",
        "description": "Comma-separated markets: h2h, spreads, totals, outrights, or player prop markets",
        "schema": {
          "type": "string",
          "default": "h2h"
        }
      },
      "dateFormat": {
        "name": "dateFormat",
        "in": "query",
        "description": "Timestamp format in response",
        "schema": {
          "type": "string",
          "enum": ["iso", "unix"],
          "default": "iso"
        }
      },
      "oddsFormat": {
        "name": "oddsFormat",
        "in": "query",
        "description": "Odds format in response",
        "schema": {
          "type": "string",
          "enum": ["decimal", "american"],
          "default": "decimal"
        }
      },
      "eventIds": {
        "name": "eventIds",
        "in": "query",
        "description": "Comma-separated event IDs to filter results",
        "schema": {
          "type": "string"
        }
      },
      "bookmakers": {
        "name": "bookmakers",
        "in": "query",
        "description": "Comma-separated list of bookmaker keys",
        "schema": {
          "type": "string"
        }
      },
      "commenceTimeFrom": {
        "name": "commenceTimeFrom",
        "in": "query",
        "description": "Filter games commencing on/after this time (ISO 8601)",
        "schema": {
          "type": "string",
          "format": "date-time"
        }
      },
      "commenceTimeTo": {
        "name": "commenceTimeTo",
        "in": "query",
        "description": "Filter games commencing on/before this time (ISO 8601)",
        "schema": {
          "type": "string",
          "format": "date-time"
        }
      },
      "date": {
        "name": "date",
        "in": "query",
        "required": true,
        "description": "Historical snapshot timestamp (ISO 8601)",
        "schema": {
          "type": "string",
          "format": "date-time"
        },
        "example": "2021-10-18T12:00:00Z"
      }
    },
    "headers": {
      "x-requests-remaining": {
        "description": "Usage credits remaining until quota resets",
        "schema": {
          "type": "integer"
        }
      },
      "x-requests-used": {
        "description": "Usage credits used since last quota reset",
        "schema": {
          "type": "integer"
        }
      },
      "x-requests-last": {
        "description": "Usage cost of the last API call",
        "schema": {
          "type": "integer"
        }
      }
    },
    "responses": {
      "Unauthorized": {
        "description": "Invalid or missing API key",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/Error"
            }
          }
        }
      },
      "RateLimited": {
        "description": "Rate limit exceeded. Space out requests.",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/Error"
            }
          }
        }
      }
    },
    "schemas": {
      "Sport": {
        "type": "object",
        "properties": {
          "key": {
            "type": "string",
            "description": "Unique sport key used in API calls",
            "example": "americanfootball_nfl"
          },
          "group": {
            "type": "string",
            "description": "Sport category",
            "example": "American Football"
          },
          "title": {
            "type": "string",
            "description": "Display name",
            "example": "NFL"
          },
          "description": {
            "type": "string",
            "description": "Sport description",
            "example": "US Football"
          },
          "active": {
            "type": "boolean",
            "description": "Whether the sport is currently in season"
          },
          "has_outrights": {
            "type": "boolean",
            "description": "Whether outright/futures markets are available"
          }
        },
        "required": ["key", "group", "title", "description", "active", "has_outrights"]
      },
      "BasicEvent": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "Unique event identifier"
          },
          "sport_key": {
            "type": "string"
          },
          "sport_title": {
            "type": "string"
          },
          "commence_time": {
            "type": "string",
            "format": "date-time"
          },
          "home_team": {
            "type": "string"
          },
          "away_team": {
            "type": "string"
          }
        },
        "required": ["id", "sport_key", "sport_title", "commence_time", "home_team", "away_team"]
      },
      "Event": {
        "allOf": [
          {
            "$ref": "#/components/schemas/BasicEvent"
          },
          {
            "type": "object",
            "properties": {
              "bookmakers": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/Bookmaker"
                }
              }
            }
          }
        ]
      },
      "ScoreEvent": {
        "allOf": [
          {
            "$ref": "#/components/schemas/BasicEvent"
          },
          {
            "type": "object",
            "properties": {
              "completed": {
                "type": "boolean"
              },
              "scores": {
                "type": "array",
                "nullable": true,
                "items": {
                  "$ref": "#/components/schemas/Score"
                }
              },
              "last_update": {
                "type": "string",
                "format": "date-time",
                "nullable": true
              }
            }
          }
        ]
      },
      "Score": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Team name"
          },
          "score": {
            "type": "string",
            "description": "Current score"
          }
        },
        "required": ["name", "score"]
      },
      "Bookmaker": {
        "type": "object",
        "properties": {
          "key": {
            "type": "string",
            "example": "draftkings"
          },
          "title": {
            "type": "string",
            "example": "DraftKings"
          },
          "last_update": {
            "type": "string",
            "format": "date-time"
          },
          "markets": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Market"
            }
          }
        },
        "required": ["key", "title", "markets"]
      },
      "Market": {
        "type": "object",
        "properties": {
          "key": {
            "type": "string",
            "description": "Market type (h2h, spreads, totals, etc.)",
            "example": "h2h"
          },
          "last_update": {
            "type": "string",
            "format": "date-time"
          },
          "outcomes": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Outcome"
            }
          }
        },
        "required": ["key", "outcomes"]
      },
      "Outcome": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Team/selection name or Over/Under"
          },
          "price": {
            "type": "number",
            "description": "Odds price (decimal or american format)"
          },
          "point": {
            "type": "number",
            "description": "Point spread or total line (for spreads/totals markets)"
          },
          "description": {
            "type": "string",
            "description": "Player name (for player prop markets)"
          }
        },
        "required": ["name", "price"]
      },
      "EventMarkets": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "sport_key": {
            "type": "string"
          },
          "sport_title": {
            "type": "string"
          },
          "commence_time": {
            "type": "string",
            "format": "date-time"
          },
          "home_team": {
            "type": "string"
          },
          "away_team": {
            "type": "string"
          },
          "bookmakers": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "key": {
                  "type": "string"
                },
                "title": {
                  "type": "string"
                },
                "markets": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "key": {
                        "type": "string"
                      },
                      "last_update": {
                        "type": "string",
                        "format": "date-time"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "Participant": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "Unique participant ID"
          },
          "full_name": {
            "type": "string",
            "description": "Team or player name"
          }
        },
        "required": ["id", "full_name"]
      },
      "HistoricalOddsResponse": {
        "type": "object",
        "properties": {
          "timestamp": {
            "type": "string",
            "format": "date-time",
            "description": "Actual snapshot timestamp"
          },
          "previous_timestamp": {
            "type": "string",
            "format": "date-time",
            "description": "Previous available timestamp"
          },
          "next_timestamp": {
            "type": "string",
            "format": "date-time",
            "description": "Next available timestamp"
          },
          "data": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Event"
            }
          }
        },
        "required": ["timestamp", "data"]
      },
      "HistoricalEventsResponse": {
        "type": "object",
        "properties": {
          "timestamp": {
            "type": "string",
            "format": "date-time"
          },
          "previous_timestamp": {
            "type": "string",
            "format": "date-time"
          },
          "next_timestamp": {
            "type": "string",
            "format": "date-time"
          },
          "data": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/BasicEvent"
            }
          }
        },
        "required": ["timestamp", "data"]
      },
      "HistoricalEventOddsResponse": {
        "type": "object",
        "properties": {
          "timestamp": {
            "type": "string",
            "format": "date-time"
          },
          "previous_timestamp": {
            "type": "string",
            "format": "date-time"
          },
          "next_timestamp": {
            "type": "string",
            "format": "date-time"
          },
          "data": {
            "$ref": "#/components/schemas/Event"
          }
        },
        "required": ["timestamp", "data"]
      },
      "Error": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          }
        }
      }
    }
  },
  "tags": [
    {
      "name": "Sports",
      "description": "Sports and participants information"
    },
    {
      "name": "Current Events",
      "description": "Live and upcoming events with odds and scores"
    },
    {
